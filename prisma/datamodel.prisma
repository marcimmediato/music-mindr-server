type User {
  id: ID! @unique
  username: String!
  email: String! @unique
  password: String!
  mindrs: [Mindr!]! @relation(name: "MindrToUser", onDelete: CASCADE)
  updatedAt: DateTime!
  createdAt: DateTime!
}

type Mindr {
  id: ID! @unique
  user: User! @relation(name: "MindrToUser", onDelete: SET_NULL)
  spotifyId: String!
  type: String!
  name: String!
  artist: String!
  note: String
}
